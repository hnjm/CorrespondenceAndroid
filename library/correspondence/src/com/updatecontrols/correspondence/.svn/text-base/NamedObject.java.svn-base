package com.updatecontrols.correspondence;

public class NamedObject {

	private String name;
	private Object value;
	
	public String getName() {
		return name;
	}

	public Object getValue() {
		return value;
	}

	public NamedObject(String name, Object value) {
		super();
		this.name = name;
		this.value = value;
	}
}
