package com.updatecontrols.correspondence.networking;

import com.updatecontrols.correspondence.strategy.AsynchronousCommunicationStrategy;

public class AsynchronousServerProxy {

	private AsynchronousCommunicationStrategy communicationStrategy;
	private int peerId;
	
	public AsynchronousServerProxy(
			AsynchronousCommunicationStrategy communicationStrategy, int peerId) {
		super();
		this.communicationStrategy = communicationStrategy;
		this.peerId = peerId;
	}

	public AsynchronousCommunicationStrategy getCommunicationStrategy() {
		return communicationStrategy;
	}

	public int getPeerId() {
		return peerId;
	}
	
}
