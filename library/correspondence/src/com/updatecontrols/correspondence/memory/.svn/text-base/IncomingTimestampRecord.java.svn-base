package com.updatecontrols.correspondence.memory;

import com.updatecontrols.correspondence.memento.FactID;
import com.updatecontrols.correspondence.memento.TimestampID;

public class IncomingTimestampRecord {
	public int peerId;
	public FactID pivotId;
	public TimestampID timestampId;

	public IncomingTimestampRecord(int peerId, FactID pivotId, TimestampID timestampId) {
		this.peerId = peerId;
		this.pivotId = pivotId;
		this.timestampId = timestampId;
	}
}